<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
		<em:id>tabscroll@mthamil</em:id>
        <em:version>20140723</em:version>
		<em:name>Tab Wheel Scroll</em:name>
        <em:description>Switch tabs by scrolling with the mouse wheel in the tab bar.</em:description>
        <em:creator>Matt Hamilton</em:creator>
		<em:homepageURL>http://github.com/mthamil/Tab-Wheel-Scroll</em:homepageURL>
        <em:type>2</em:type>
		<em:bootstrap>true</em:bootstrap>
		<em:unpack>false</em:unpack>
        <em:optionsURL>chrome://tabscroll/content/options.xul</em:optionsURL>
		<em:optionsType>2</em:optionsType>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>17.0</em:minVersion>
                <em:maxVersion>30.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        
        <!-- SeaMonkey -->
        <em:targetApplication>
          <Description>
            <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
            <em:minVersion>2.14.*</em:minVersion>
            <em:maxVersion>2.*.*</em:maxVersion>
          </Description>
        </em:targetApplication>
        
        <!-- Thunderbird -->
        <em:targetApplication>
          <Description>
            <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
            <em:minVersion>17.0</em:minVersion>
            <em:maxVersion>24.*.*</em:maxVersion>
          </Description>
        </em:targetApplication>

